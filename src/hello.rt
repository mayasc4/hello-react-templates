<div>
<h2>Hello React Templates</h2>
    <label>
        <input type="text" valueLink="{this.linkState('newFieldName')}" />
    </label>
    <button onClick="()=>this.handleClick()">Submit</button>
    <div rt-repeat="field in this.state.fields">
        <input onChange="(e)=>this.handleFieldChange(e)" key={field}/>
    </div>

</div>
